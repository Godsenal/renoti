  },
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.config.dev.js --hot",
    "build:clean": "rm -rf ./docs",
    "build": "npm run build:clean && NODE_ENV=production webpack --config ./webpack.config.prod.js",
